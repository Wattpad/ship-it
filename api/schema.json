{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "items"
  ],
  "properties": {
    "items": {
      "$id": "#/properties/items",
      "type": "array",
      "title": "The Items Schema",
      "items": {
        "$id": "#/properties/items/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "name",
          "created",
          "lastDeployed",
          "owner",
          "autoDeploy",
          "code",
          "build",
          "monitoring",
          "artifacts",
          "deployment"
        ],
        "properties": {
          "name": {
            "$id": "#/properties/items/items/properties/name",
            "type": "string",
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "homeslice"
            ],
            "pattern": "^(.*)$"
          },
          "created": {
            "$id": "#/properties/items/items/properties/created",
            "type": "string",
            "title": "The Created Schema",
            "default": "",
            "examples": [
              "2019-06-13T14:09:47.781282"
            ],
            "pattern": "^(.*)$"
          },
          "lastDeployed": {
            "$id": "#/properties/items/items/properties/lastDeployed",
            "type": "string",
            "title": "The Lastdeployed Schema",
            "default": "",
            "examples": [
              "2019-06-13T14:09:47.78Z"
            ],
            "pattern": "^(.*)$"
          },
          "owner": {
            "$id": "#/properties/items/items/properties/owner",
            "type": "object",
            "title": "The Owner Schema",
            "required": [
              "team",
              "slack"
            ],
            "properties": {
              "team": {
                "$id": "#/properties/items/items/properties/owner/properties/team",
                "type": "string",
                "title": "The Team Schema",
                "default": "",
                "examples": [
                  "squad-cd"
                ],
                "pattern": "^(.*)$"
              },
              "slack": {
                "$id": "#/properties/items/items/properties/owner/properties/slack",
                "type": "string",
                "title": "The Slack Schema",
                "default": "",
                "examples": [
                  "#squad-cd"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "autoDeploy": {
            "$id": "#/properties/items/items/properties/autoDeploy",
            "type": "boolean",
            "title": "The Autodeploy Schema",
            "default": false,
            "examples": [
              true
            ]
          },
          "code": {
            "$id": "#/properties/items/items/properties/code",
            "type": "object",
            "title": "The Code Schema",
            "required": [
              "github",
              "ref"
            ],
            "properties": {
              "github": {
                "$id": "#/properties/items/items/properties/code/properties/github",
                "type": "string",
                "title": "The Github Schema",
                "default": "",
                "examples": [
                  "https://github.com/Wattpad/highlander/wattpad/src/services/homeslice"
                ],
                "pattern": "^(.*)$"
              },
              "ref": {
                "$id": "#/properties/items/items/properties/code/properties/ref",
                "type": "string",
                "title": "The Ref Schema",
                "default": "",
                "examples": [
                  "adf098ad00a8d76d5ad5ad4ada5dad4ad"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "build": {
            "$id": "#/properties/items/items/properties/build",
            "type": "object",
            "title": "The Build Schema",
            "required": [
              "travis"
            ],
            "properties": {
              "travis": {
                "$id": "#/properties/items/items/properties/build/properties/travis",
                "type": "string",
                "title": "The Travis Schema",
                "default": "",
                "examples": [
                  ""
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "monitoring": {
            "$id": "#/properties/items/items/properties/monitoring",
            "type": "object",
            "title": "The Monitoring Schema",
            "required": [
              "datadog",
              "sumologic"
            ],
            "properties": {
              "datadog": {
                "$id": "#/properties/items/items/properties/monitoring/properties/datadog",
                "type": "object",
                "title": "The Datadog Schema",
                "required": [
                  "dashboard",
                  "monitors"
                ],
                "properties": {
                  "dashboard": {
                    "$id": "#/properties/items/items/properties/monitoring/properties/datadog/properties/dashboard",
                    "type": "string",
                    "title": "The Dashboard Schema",
                    "default": "",
                    "examples": [
                      "https://app.datadoghq.com/dashboard/4px-qaj-tnc/home-v2?tile_size=m"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "monitors": {
                    "$id": "#/properties/items/items/properties/monitoring/properties/datadog/properties/monitors",
                    "type": "string",
                    "title": "The Monitors Schema",
                    "default": "",
                    "examples": [
                      "https://app.datadoghq.com/monitors/manage?q=home"
                    ],
                    "pattern": "^(.*)$"
                  }
                }
              },
              "sumologic": {
                "$id": "#/properties/items/items/properties/monitoring/properties/sumologic",
                "type": "string",
                "title": "The Sumologic Schema",
                "default": "",
                "examples": [
                  "https://service.us2.sumologic.com/ui/#/search/KRBpz5OodF4HcTasdIEYkuhguVAEennEj7xIV8ke"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "artifacts": {
            "$id": "#/properties/items/items/properties/artifacts",
            "type": "object",
            "title": "The Artifacts Schema",
            "required": [
              "docker",
              "chart"
            ],
            "properties": {
              "docker": {
                "$id": "#/properties/items/items/properties/artifacts/properties/docker",
                "type": "object",
                "title": "The Docker Schema",
                "required": [
                  "image",
                  "tag"
                ],
                "properties": {
                  "image": {
                    "$id": "#/properties/items/items/properties/artifacts/properties/docker/properties/image",
                    "type": "string",
                    "title": "The Image Schema",
                    "default": "",
                    "examples": [
                      "723255503624.dkr.ecr.us-east-1.amazonaws.com/homeslice"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "tag": {
                    "$id": "#/properties/items/items/properties/artifacts/properties/docker/properties/tag",
                    "type": "string",
                    "title": "The Tag Schema",
                    "default": "",
                    "examples": [
                      "adf098ad00a8d76d5ad5ad4ada5dad4ad"
                    ],
                    "pattern": "^(.*)$"
                  }
                }
              },
              "chart": {
                "$id": "#/properties/items/items/properties/artifacts/properties/chart",
                "type": "object",
                "title": "The Chart Schema",
                "required": [
                  "path",
                  "version"
                ],
                "properties": {
                  "path": {
                    "$id": "#/properties/items/items/properties/artifacts/properties/chart/properties/path",
                    "type": "string",
                    "title": "The Path Schema",
                    "default": "",
                    "examples": [
                      "https://charts.wattpadhq.com/homeslice"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "version": {
                    "$id": "#/properties/items/items/properties/artifacts/properties/chart/properties/version",
                    "type": "string",
                    "title": "The Version Schema",
                    "default": "",
                    "examples": [
                      "1.2.3"
                    ],
                    "pattern": "^(.*)$"
                  }
                }
              }
            }
          },
          "deployment": {
            "$id": "#/properties/items/items/properties/deployment",
            "type": "object",
            "title": "The Deployment Schema",
            "required": [
              "status",
              "resources"
            ],
            "properties": {
              "status": {
                "$id": "#/properties/items/items/properties/deployment/properties/status",
                "type": "string",
                "title": "The Status Schema",
                "default": "",
                "examples": [
                  "deploying"
                ],
                "pattern": "^(.*)$"
              },
              "resources": {
                "$id": "#/properties/items/items/properties/deployment/properties/resources",
                "type": "array",
                "title": "The Resources Schema",
                "items": {
                  "$id": "#/properties/items/items/properties/deployment/properties/resources/items",
                  "type": "object",
                  "title": "The Items Schema",
                  "required": [
                    "kind",
                    "metadata",
                    "replicas",
                    "availableReplicas"
                  ],
                  "properties": {
                    "kind": {
                      "$id": "#/properties/items/items/properties/deployment/properties/resources/items/properties/kind",
                      "type": "string",
                      "title": "The Kind Schema",
                      "default": "",
                      "examples": [
                        "Deployment"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "metadata": {
                      "$id": "#/properties/items/items/properties/deployment/properties/resources/items/properties/metadata",
                      "type": "object",
                      "title": "The Metadata Schema",
                      "required": [
                        "name"
                      ],
                      "properties": {
                        "name": {
                          "$id": "#/properties/items/items/properties/deployment/properties/resources/items/properties/metadata/properties/name",
                          "type": "string",
                          "title": "The Name Schema",
                          "default": "",
                          "examples": [
                            "homeslice"
                          ],
                          "pattern": "^(.*)$"
                        }
                      }
                    },
                    "replicas": {
                      "$id": "#/properties/items/items/properties/deployment/properties/resources/items/properties/replicas",
                      "type": "integer",
                      "title": "The Replicas Schema",
                      "default": 0,
                      "examples": [
                        1
                      ]
                    },
                    "availableReplicas": {
                      "$id": "#/properties/items/items/properties/deployment/properties/resources/items/properties/availableReplicas",
                      "type": "integer",
                      "title": "The Availablereplicas Schema",
                      "default": 0,
                      "examples": [
                        1
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
